{
	"fio": {
		"label": "FilesIO",
		"uis":[
			{
				"module": "fio",
				"list": [
					{
						"type": "UIButton",
						"callback": "saveLocal",
						"key": "s",
						"params": {
							"text": "Save Local"
						}
					},
					{
						"type": "UIButton",
						"callback": "saveFile",
						"key": "alt-s",
						"params": {
							"text": "Save File"
						}
					},
					{
						"type": "UIFile",
						"callback": "load",
						"key": "o",
						"params": {
							"text": "Load File",
							"promptDefault": "compositions"
						}
					},
					{
						"type": "UIButton",
						"callback": "clear",
						"params": {
							"text": "Clear Local"
						}
					},
					{
						"type": "UIFile",
						"callback": "loadMidi",
						"params": {
							"text": "Load Midi",
							"promptDefault": "compositions",
							"fileType": "audio/midi"
						}

					}
				]
			}
		]
	},
	"playback": {
		"label": "Play Back",
		"uis": [
			{
				"module": "composition",
				"list": [
					{
						"type": "UIButton",
						"callback": "play",
						"key": "space",
						"params": {
							"text": "Play",
							"args": [false]
						}
					},
					{
						"type": "UIButton",
						"callback": "stop",
						"key": "ctrl-space",
						"params": {
							"text": "Stop"
						}
					},
					{
						"type": "UIButton",
						"callback": "play",
						"key": "r",
						"params": {
							"text": "Record",
							"args": [true]
						}
					},
					{
						"type": "UIButton",
						"callback": "mutate",
						"key": "d",
						"params": {
							"text": "Mutate"
						}
					},
					{
						"row": true,
						"type": "UIToggleCheck",
						"label": "Metro",
						"toggle": "useMetro",
						"key": "m",
						"face": "useMetro"
					},
					{
						"row": true,
						"type": "UIElement",
						"id": "mutation-count",
						"face": "mutationCount",
						"ignoreSettings": true,
						"params": {
							"text": "Mutation 0"
						}
					}
				]
			}
		]
	},
	"composition": {
		"label": "Composition",
		"addPanel": true,
		"uis": [
			{
				"module": "composition",
				"list": [
					{
						"type": "UIText",
						"label": "Title",
						"string": "title",
						"face": "title",
						"params": {
							"id": "title"
						}
					},
					{
						"row": true,
						"type": "UIText",
						"string": "tonic",
						"face": "tonic",
						"label": "Tonic",
						"fromModule": {
							"placeholder": "tonic"
						}
					},
					{
						"type": "UIText",
						"string": "transform",
						"face": "transform",
						"label": "Tonic Transform",
						"fromModule": {
							"placeholder": "transform"
						}
					},
					{
						"type": "UINumberStep",
						"number": "bpm",
						"face": "bpm",
						"label": "BPM",
						"fromModule": {
							"value": "bpm"
						},
						"params": {
							"min": 60,
							"max": 250
						}
					},
					{
						"type": "UISelect",
						"label": "Samples/Voice",
						"string": "samples",
						"face": "samples",
						"params": {
							"selected": "synth",
							"options": [
								{ "value": "../samples/choir/", "text": "Choir" },
								{ "value": "synth", "text": "FMSynth" }
							]
						}
					},
					{
						"type": "UISelect",
						"label": "(Default) Note Duration",
						"string": "startDuration",
						"face": "startDuration",
						"params": {
							"selected": "4n",
							"options": [
								{ "value": "1n", "text": "Whole 1n" },
								{ "value": "2n", "text": "Half 2n" },
								{ "value": "4n", "text": "Quarter 4n" },
								{ "value": "8n", "text": "Eighth 8n" }
							]
						}
					},
					{
						"type": "UIRow",
						"params": {
							"class": "break"
						}
					},
					{
						"type": "UILabel",
						"params": {
							"text": "Scale (intervals)"
						}
					},
					{
						"type": "UIRow",
						"k": "scaleRow",
						"class": "break"
					}
				]
			}
		]
	},
	"melody": {
		"label": "Melody",
		"addPanel": true,
		"uis": [
			{
				"module": "composition",
				"list": [
					{
						"type": "UIRow",
						"k": "melodyInput",
						"params": {
							"class": "break"
						}
					},
					{
						"type": "UINumberStep",
						"face": "uiScale",
						"callback": "setUIScale",
						"label": "Scale",
						"fromModule": {
							"value": "uiScale"
						}
					},
					{

						"type": "UINumberStep",
						"face": "noteWidth",
						"callback": "setNoteWidth",
						"label": "Note Width",
						"fromModule": {
							"value": "noteWidth"
						}
					},
					{
						"type": "UIButton",
						"key": "0",
						"callback": "clear",
						"params": {
							"text": "Clear"
						}
					},
					{
						"type": "UINumberStep",
						"number": "currentPart",
						"face": "currentPart",
						"label": "Part"
					},
					{
						"type": "UIText",
						"k": "noteInput",
						"params": {
							"placeholder": "C4"
						}
					},
					{
						"type": "UIText",
						"k": "durationInput",
						"params": {
							"placeholder": "4n"
						}
					},
					{
						"type": "UIButton",
						"callback": "addNote",
						"key": "+",
						"params": {
							"text": "+"
						}
					},
					{
						"type": "UIRow",
						"k": "melody",
						"params": {
							"class": "break",
							"id": "melody"
						}
					}
				]
			}
		]
	},
	"params": {
		"label": "Doodoo Params",
		"addPanel": true,
		"uis": [
			{
				"module": "params",
				"list": [
					{
						"type": "UIRow",
						"k": "doodooParams",
						"params": {
							"id": "doodoo-params"
						}
					}
				]
			}
		]
	},
	"loops": {
		"label": "Start Loops",
		"addPanel": true,
		"uis": [
			{
				"module": "params",
				"list": [
					{
						"type": "UIRow",
						"k": "startLoops",
						"params": {
							"id": "start-loops-row"
						}
					}
				]
			}
		]
	}
}